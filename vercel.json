{
	"rewrites": [
		{
			"source": "/admin/",
			"destination": "https://api.uplionsserve.org/admin/"
		},
		{
			"source": "/admin/:match*",
			"destination": "https://api.uplionsserve.org/admin/:match*"
		},
		{
			"source": "/childhood-cancer/",
			"destination": "https://uplionsserve-cc.vercel.app/childhood-cancer/"
		},
		{
			"source": "/childhood-cancer/:match*",
			"destination": "https://uplionsserve-cc.vercel.app/childhood-cancer/:match*"
		},
		{
			"source": "/hunger/",
			"destination": "https://uplionsserve-hunger.vercel.app/hunger"
		},
		{
			"source": "/hunger/:match*",
			"destination": "https://uplionsserve-hunger.vercel.app/hunger/:match*"
		},
		{
			"source": "/",
			"destination": "https://uplionsserve.vercel.app/"
		},
		{
			"source": "/:match((?!admin).*)",
			"destination": "https://uplionsserve.vercel.app/:match*"
		}
	]
}
