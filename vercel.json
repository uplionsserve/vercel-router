{
	"redirects": [
		{
			"source": "/admin",
			"destination": "https://api.uplionsserve.org/admin",
			"permanent": false
		},
		{
			"source": "/(cc|childhoodcancer)",
			"destination": "https://uplionsserve.org/childhood-cancer",
			"permanent": true
		},
		{
			"source": "/(cc|childhoodcancer)/:match*",
			"destination": "https://uplionsserve.org/childhood-cancer/:match*",
			"permanent": true
		},
		{
			"source": "/(dad|daa|defenseagainstdiabetes|defense-against-diabetes)",
			"destination": "https://uplionsserve.org/diabetes-awareness-and-action",
			"permanent": true
		},
		{
			"source": "/(dad|daa|defenseagainstdiabetes|defense-against-diabetes)/:match*",
			"destination": "https://uplionsserve.org/diabetes-awareness-and-action/:match*",
			"permanent": true
		}
	],
	"rewrites": [
		{
			"source": "/childhood-cancer",
			"destination": "https://uplionsserve-cc.vercel.app/childhood-cancer"
		},
		{
			"source": "/childhood-cancer/:match*",
			"destination": "https://uplionsserve-cc.vercel.app/childhood-cancer/:match*"
		},
		{
			"source": "/diabetes-awareness-and-action",
			"destination": "https://uplionsserve-dad.vercel.app/diabetes-awareness-and-action"
		},
		{
			"source": "/diabetes-awareness-and-action/:match*",
			"destination": "https://uplionsserve-dad.vercel.app/diabetes-awareness-and-action/:match*"
		},
		{
			"source": "/hunger",
			"destination": "https://uplionsserve-hunger.vercel.app/hunger"
		},
		{
			"source": "/hunger/:match*",
			"destination": "https://uplionsserve-hunger.vercel.app/hunger/:match*"
		},
		{
			"source": "/",
			"destination": "https://uplionsserve.vercel.app/"
		},
		{
			"source": "/:match((?!admin).*)",
			"destination": "https://uplionsserve.vercel.app/:match*"
		}
	]
}
